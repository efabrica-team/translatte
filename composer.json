{
  "name": "efabrica/translatte",
  "type": "library",
  "description": "Translator for Nette framework",
  "keywords": ["translator", "nette"],
  "license": "MIT",
  "require": {
    "php": ">= 7.1",
    "nette/utils": "^3.0",
    "nette/finder": "^2.5",
    "nette/neon": "^3.0"
  },
  "require-dev": {
    "nette/application": "^3.0",
    "nette/di": "^3.0"
  },
  "suggest": {
    "nette/application": "For Nette bridge - TranslationExtension",
    "nette/di": "For Nette bridge - TranslationExtension"
  },
  "autoload": {
    "classmap": ["src/"]
  },
  "autoload-dev": {
    "classmap": ["tests/"]
  }
}
